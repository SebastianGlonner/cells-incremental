{
    "name": "template-svelte",
    "description": "A Phaser 3 project template that demonstrates Svelte communication and uses Vite for bundling.",
    "version": "1.1.2",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/phaserjs/template-svelte.git"
    },
    "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "bugs": {
        "url": "https://github.com/phaserjs/template-svelte/issues"
    },
    "homepage": "https://github.com/phaserjs/template-svelte#readme",
    "keywords": [
        "phaser",
        "phaser3",
        "svelte",
        "vite"
    ],
    "scripts": {
        "dev": "vite --config vite/config.dev.mjs",
        "build": "vite build --config vite/config.prod.mjs",
        "dev-nolog": "vite --config vite/config.dev.mjs",
        "build-nolog": "vite build --config vite/config.prod.mjs",
        "preview": "vite preview --outDir build",
        "prepare": "npm run smui-theme",
        "smui-theme": "smui-theme compile static/smui.css -i src/theme"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.3",
        "@mdi/js": "^7.4.47",
        "@smui/icon-button": "^8.0.0-beta.3",
        "@smui/top-app-bar": "^8.0.0-beta.3",
        "@sveltejs/adapter-auto": "3.2.0",
        "@sveltejs/adapter-static": "3.0.1",
        "@sveltejs/kit": "^2.15.1",
        "@sveltejs/vite-plugin-svelte": "^5.0.2",
        "eslint": "^9.7.0",
        "eslint-plugin-svelte": "^2.36.0",
        "globals": "^15.0.0",
        "sass": "^1.83.0",
        "smui-theme": "^8.0.0-beta.3",
        "svelte": "^5.16.0",
        "svelte-check": "^4.0.0",
        "svelte-preprocess": "^6.0.3",
        "terser": "^5.37.0",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.0.0",
        "vite": "^6.0.0",
        "vitest": "^2.0.4"
    },
    "type": "module",
    "dependencies": {
        "@capacitor/android": "^6.2.0",
        "@capacitor/cli": "^6.2.0",
        "@capacitor/core": "^6.2.0",
        "@capacitor/preferences": "^6.0.3",
        "bulma": "^1.0.3",
        "phaser": "^3.87.0"
    }
}
